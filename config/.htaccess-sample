<Files ~ "\.ini$">
Order allow,deny
Deny from all
</Files>

RewriteEngine On
RewriteBase /

RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [S=51]

RewriteRule (.*[^/])$ $1/ [R=301,QSA]

RewriteRule ^run/$ index.php?action=run [QSA,L]
RewriteRule ^run/(.*)/?$ index.php?action=run&item=$1 [QSA,L]
RewriteRule ^login/$ index.php?action=login [QSA,L]
RewriteRule ^logout/$ index.php?action=logout [QSA,L]
RewriteRule ^signup/$ index.php?action=signup [QSA,L]
RewriteRule ^scores/$ index.php?action=scores [QSA,L]
RewriteRule ^user/(.*)/?$ index.php?action=tinder&item=$1 [QSA,L]
RewriteRule ^job/(.*)/?$ index.php?action=jobstatus&item=$1 [QSA,L]
